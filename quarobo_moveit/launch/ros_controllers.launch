<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find quarobo_moveit)/config/ros_controllers.yaml" command="load"/>

  <!-- Load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="fr_joint_trajectory_controller fl_joint_trajectory_controller rr__joint_trajectory_controller rl__joint_trajectory_controller "/>

</launch>
